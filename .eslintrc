{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "standard",
  "plugins": [
    "svelte3"
  ],
  "rules": {
    "import/first": "off",
    "semi": [2, "always"],
    "comma-dangle": ["error", "always-multiline"]
  },
  "settings": {

  }
}
